{
    "name": "simbiat/simbiat.ru",
    "description": "Package for simbiat.dev",
    "prefer-stable": true,
    "version": "1.0.0",
    "minimum-stability": "dev",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Dmitrii Kustov",
            "email": "support@simbiat.dev",
            "homepage": "https://www.simbiat.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "matomo/device-detector": "^6",
        "symfony/html-sanitizer": "^7.3",
        "symfony/mailer": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "twig/twig": "^3",
        "twig/extra-bundle": "^3",
        "twig/cssinliner-extra": "^3",
        "league/html-to-markdown": "^5.1.1",
        "geoip2/geoip2": "^3",
        "tinymce/tinymce": "^7",
        "vlucas/phpdotenv": "^5",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-apcu": "*",
        "ext-zlib": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-gd": "*",
        "ext-brotli": "*",
        "ext-zstd": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Simbiat\\": [
                "./lib/CuteBytes/src",
                "./lib/DDCIcons/src",
                "./lib/HTMLCache/src",
                "./lib/SafeFileName/src",
                "./lib/SandClock/src"
            ],
            "Simbiat\\Arrays\\": [
                "./lib/ArrayHelpers/src"
            ],
            "Simbiat\\BIC\\": [
                "./lib/BIC/AccountKeying/src",
                "./lib/BIC/Tracker/src"
            ],
            "Simbiat\\Database\\": [
                "./lib/Database/Binder/src",
                "./lib/Database/Manager/src",
                "./lib/Database/Pool/src",
                "./lib/Database/Query/src",
                "./lib/Database/Installer/src"
            ],
            "Simbiat\\FFXIV\\": [
                "./lib/FFXIV/Lodestone/src",
                "./lib/FFXIV/Tracker/src"
            ],
            "Simbiat\\HTTP\\": [
                "./lib/HTTP/PrettyURL/src",
                "./lib/HTTP/SSE/src"
            ],
            "Simbiat\\HTML\\": [
                "./lib/HTML/Cut/src",
                "./lib/HTML/nl2tag/src"
            ],
            "Simbiat\\Database\\Maintainer\\": "./lib/Database/Maintainer/src",
            "Simbiat\\HTML\\ArrayToTable\\": "./lib/array2table/src",
            "Simbiat\\Cron\\": "./lib/Cron/src",
            "Simbiat\\http20\\": "./lib/http20/src",
            "Simbiat\\Website\\": "./lib/Website"
        }
    },
    "config": {
        "sort-packages": true,
        "store-auths": true,
        "preferred-install": "source",
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "htaccess-protect": true,
        "platform-check": true
    }
}