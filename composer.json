{
    "name": "simbiat/simbiat.ru",
    "description": "Package for simbiat.dev",
    "prefer-stable": true,
    "version": "1.0.0",
    "minimum-stability": "dev",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Dmitry Kustov",
            "email": "simbiat@outlook.com",
            "homepage": "https://www.simbiat.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "matomo/device-detector": "^6",
        "twig/twig": "^3",
        "symfony/html-sanitizer": "^6",
        "sendgrid/sendgrid": "^8 ",
        "ipinfo/ipinfo": "^2",
        "tinymce/tinymce": "^7",
        "vlucas/phpdotenv": "^5",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-apcu": "*",
        "ext-zlib": "*",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-openssl": "*",
        "ext-gd": "*",
        "ext-brotli": "*",
        "ext-zstd": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Simbiat\\": [
                "./lib/AccountKeying/src",
                "./lib/ArrayHelpers/src",
                "./lib/CuteBytes/src",
                "./lib/HTMLCut/src",
                "./lib/Lodestone/src",
                "./lib/nl2tag/src",
                "./lib/optimizeTables/src",
                "./lib/SafeFileName/src",
                "./lib/SandClock/src"
            ],
            "Simbiat\\Cron\\": "./lib/Cron/src",
            "Simbiat\\Database\\": "./lib/Database/src",
            "Simbiat\\http20\\": "./lib/http20/src",
            "Simbiat\\Website\\": "./lib/Website"
        }
    },
    "config": {
        "sort-packages": true,
        "store-auths": true,
        "platform": {
            "php": "8.3"
        },
        "preferred-install": "source",
        "optimize-autoloader": true,
        "classmap-authoritative": true,
        "htaccess-protect": true,
        "platform-check": true
    }
}