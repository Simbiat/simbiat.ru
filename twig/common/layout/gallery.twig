<gallery-overlay role="dialog" aria-label="Gallery overlay" class="hidden">
    <gallery-close><input id="closeGalleryIcon" class="navIcon" alt="Close gallery" data-tooltip="Close gallery" type="image" src="/img/close.svg"></gallery-close>
    <div id="galleryGrid">
        <gallery-prev>&#10096;</gallery-prev>
        <div id="galleryNameBlock"><span id="galleryName">Name</span><a id="galleryNameLink" href="" target="_blank"><img loading="lazy" decoding="async" class="linkIcon" alt="Open in new tab" src="/img/newtab.svg"></a></div>
        <gallery-image><img id="galleryLoadedImage" loading="lazy" decoding="async" alt="'+name+'" src=""></gallery-image>
        <div id="galleryCounter"><span id="galleryCurrent">0</span><span id="gallerySlash">/</span><span id="galleryTotal">0</span></div>
        <gallery-next>&#10097;</gallery-next>
    </div>
</gallery-overlay>
