<html-templates>
    {{ include('common/elements/snackbar.twig') }}
    {% if serviceName == 'uc' %}
        {% if subServiceName == 'emails' %}
            <template id="email_row">
                {{ include('usercontrol/emailrow.twig', with_context = false) }}
            </template>
        {% elseif subServiceName == 'avatars' %}
            <template id="avatar_item">
                {{ include('usercontrol/avataritem.twig', with_context = false) }}
            </template>
        {% endif %}
    {% endif %}
</html-templates>
